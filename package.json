{
  "name": "chipp-sfdx-plugins",
  "description": "sfdx plugins by Clay Chipps",
  "version": "0.1.0",
  "author": "@ctchipps",
  "bugs": "https://github.com/ctchipps/chipp-sfdx-plugins/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "^1.2.2",
    "@salesforce/command": "^3.0.3",
    "@salesforce/core": "^2.1.6",
    "@salesforce/kit": "^1.2.1",
    "@salesforce/ts-types": "^1.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.0",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/test": "^1.2.4",
    "@salesforce/dev-config": "^1.4.4",
    "@types/node": "^10.12.27",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/ctchipps/chipp-sfdx-plugins",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ctchipps/chipp-sfdx-plugins",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
